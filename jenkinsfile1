pipeline   //declarative pipelin starts with pipeline
{
    agent any //run job on any executor or build VM
    stages
    {
        stage('print a message')
        {
            steps { echo 'scm checkout'}
        }
 stage('print a message')
        {
            steps { echo 'code building'}
        }
 stage('print a message')
        {
            steps { echo 'code deployed'}
        }
    }
}
